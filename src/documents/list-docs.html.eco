---
layout: default
---

<div class="page" id="page-docs">

  <div class="docs">

    <h1>Documentation</h1>

    <ul>

    <% for docs in @getCollection('documents').sortArray('title':1): %>
      <% if docs.url.indexOf('/docs') == 0: %>
          <li><a href="<%= docs.url %>"><%= docs.title %></a></li>
      <% end %>
    <% end %>

    </ul>

  </div>

</div>